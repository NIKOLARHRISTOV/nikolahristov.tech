---
import Heading from "@Component/Readme/Heading.astro";
import Row from "@Component/Readme/Row.astro";

const Readme = (
	await import("@Page/Raw/Readme/PlayForm/Round/README.md")
).rawContent();
---

<>
	<table>
		<Heading
			DarkIcon="HTTPS://playform.ltd/Image/GitHub/Round/32x32.png"
			LightIcon="HTTPS://playform.ltd/Image/GitHub/Round/32x32.png"
			Text="Round —"
		/>

		<Row Package={new Set(["GitHub:PlayForm/Round"])} />
	</table>

	<Fragment set:html={Readme} />
</>
